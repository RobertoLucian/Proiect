package Proiect;

public class AsusROG implements PCBuilder{

private PC pc;
	
public AsusROG()
{
	this.pc=new PC();
}


@Override
public void buildProcesor() {
	// TODO Auto-generated method stub
	pc.setProcesor("AMD Ryzen 7 5800X");
}

@Override
public void buildPlacaVideo() {
	// TODO Auto-generated method stub
	pc.setPlacaVideo("GeForce RTX 3060");
}

@Override
public void buildPlacaDeBaza() {
	// TODO Auto-generated method stub
	pc.setPlacaDeBaza("AMD B550");
}

@Override
public void buildRam() {
	// TODO Auto-generated method stub
	pc.setRam("64 GB");
}

@Override
public void buildTipRacire() {
	// TODO Auto-generated method stub
	pc.setTipRacire("3 x Cooler");
}

@Override
public void buildSursa() {
	// TODO Auto-generated method stub
	pc.setSursa("Dedicata 500W");
}

public void buildStocare() {
	pc.setStocare("SSD 512GB");
}

public PC getPC()
{
		return this.pc;
}


@Override
public void buildProcesor(String Procesor) {
	
	
}


@Override
public void buildPlacaVideo(String PlacaV) {
	
	
}


@Override
public void buildPlacaDeBaza(String PlacaB) {
	
	
}


@Override
public void buildRam(String Ram) {
	
	
}


@Override
public void buildTipRacire(String TipRacire) {
	
	
}


@Override
public void buildSursa(String Sursa) {

	
}


@Override
public void buildStocare(String Stocare) {
		
}
}
